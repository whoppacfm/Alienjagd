i=1

for p=0 to pages

	create bitmap 1,640,256
	set current bitmap 1
	y=0

	for c=0 to 8
		read text$
		if text$="*" then text$=""
		center text 320,y,text$
		y=y+28
	next c

	get image i,192,0,448,256
	delete bitmap 1
	inc i

next p

startexture=i

create bitmap 1,256,256
set current bitmap 1

ink rgb(255,255,255),rgb(0,0,0)

for a=0 to 100
	dot x,y
	x=rnd(254)+1
	y=rnd(254)+1
next a

get image startexture,0,0,256,256
delete bitmap 1


dec i
z=0

for a=1 to i

	make object cube a,50
	scale object a,100,0,150
	texture object a,a
	ghost object on a
	position object a,0,0,z
	z=z+74

next a


c#=-50

position camera 0,1,c#

yrotate camera 180
xrotate camera 20


texture backdrop startexture
fog color rgb(0,0,0)
fog distance 250
fog on


vanish=(pages*100)+100


update#=0.25

do

	position camera 0,20,c#
	c#=c#+update#
	if c#>vanish then c#=-50

	if upkey() then update#=update#+0.25
	if downkey() then update#=update#-0.25

	sync

loop

